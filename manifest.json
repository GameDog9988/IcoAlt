{
 "manifest_version": 3,
 "name": "IcoAlt",
 "version": "1.0",
 "description": "Adds accessibility labels to social links that don't have one!",
 "content_scripts": [
  {
   "matches": ["<all_urls>"],
   "js": ["script.js"],
   "css": ["styles.css"]
  }
 ],
  "action": {
    "default_icon": {
    "16": "/images/favicon-16x16.png",
    "32": "/images/favicon-32x32.png"
    }
  },
   "icons": {
    "16": "/images/favicon-16x16.png",
    "32": "/images/favicon-32x32.png"
  }
}