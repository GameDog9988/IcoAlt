{
 "manifest_version": 3,
 "name": "IcoAlt",
 "version": "1.0",
 "description": "Adds accessibility labels to social links that don't have one!",
  "action": {
    "default_icon": {
      "16": "/images/favicon-16x16.png",
      "32": "/images/favicon-32x32.png"
    },
    "default_title": "Click to add aria-labels to social media links"
  },
   "icons": {
    "16": "/images/favicon-16x16.png",
    "32": "/images/favicon-32x32.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting"
  ],
    "background": {
    "service_worker": "background.js"
  }
}