{
 "manifest_version": 3,
 "name": "Belly Button",
 "version": "1.0",
 "description": "Tools to inspect buttons for developers and accessibility engineers.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/favicon-16x16.png",
      "32": "/images/favicon-32x32.png",
      "48": "/images/TRexEmoji-128.png",
      "128": "/images/TRexEmoji-128.png"
    },
    "default_title": "Click to add aria-labels to social media links"
  },
   "icons": {
    "16": "/images/favicon-16x16.png",
    "48": "/images/TRexEmoji-128.png",
    "128": "/images/TRexEmoji-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}